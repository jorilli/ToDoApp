<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ToDoApp.HomeMenu"
             Title="HomeMenu">
    <CollectionView>
        <CollectionView.Header>
            <StackLayout BackgroundColor="#dcdede">
                <ImageButton Source="home.png"
                             Clicked="Home_Clicked"/>
                <ImageButton Source="profil.png"
                             Clicked="Profil_Clicked"/>
                <Image Source="LogoDunkel.png" />
                <ImageButton Source="exit.png"
                             Clicked="Exit_Clicked"/>
            </StackLayout>
        </CollectionView.Header>
    </CollectionView>
</ContentPage>